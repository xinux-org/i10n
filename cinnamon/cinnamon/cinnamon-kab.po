msgstr "Asefrak n tnezmart"

#. power@cinnamon.org->metadata.json->description
msgid "Cinnamon power management applet"
msgstr "Taplidt n Cinnamon i usefrek n tnezmart"

#. power@cinnamon.org->settings-schema.json->labelinfo->options
msgid "Show percentage"
msgstr "Sken afmiḍi"

#. power@cinnamon.org->settings-schema.json->labelinfo->options
msgid "Show time remaining"
msgstr "Sken akud i d-yegran"

#. power@cinnamon.org->settings-schema.json->labelinfo->options
msgid "Show percentage and time remaining"
msgstr "Sken afmiḍ n wakud i d-yegran"

#. power@cinnamon.org->settings-schema.json->labelinfo->options
msgid "Hide label"
msgstr "Ffer tabzimt"

#. power@cinnamon.org->settings-schema.json->showmulti->description
msgid "Always show all batteries"
msgstr "Yalas skan tibatriyin merra"

#. power@cinnamon.org->settings-schema.json->showmulti->tooltip
msgid ""
"Check this to always show multiple batteries in the panel, regardless of "
"whether one of them is marked primary."
msgstr ""
"Fren wagi iwakken ad ttwaseknent aṭas n tbatriyin deg ugalis, ama ttusbadunt "
"d tizwaranin neɣ ala."

