#: usr/share/linuxmint/mintlocale/install_remove.ui.h:5
msgid "Remove the selected language"
msgstr "Премахване на избрания език"

#: usr/share/linuxmint/mintlocale/install_remove.ui.h:6
msgid "Install language packs"
msgstr "Инсталиране на езикови пакети"

