#: cinnamon-session/csm-manager.c:1490 cinnamon-session/csm-manager.c:2271
msgid "Not responding"
msgstr "Não respondendo"

#: cinnamon-session/csm-process-helper.c:123
#, c-format
msgid "Exited with code %d"
msgstr "Saiu com o código %d"

#: cinnamon-session/csm-process-helper.c:128
#, c-format
msgid "Killed by signal %d"
msgstr "Morto pelo sinal %d"

#: cinnamon-session/csm-process-helper.c:133
#, c-format
msgid "Stopped by signal %d"
msgstr "Parado pelo sinal %d"

#: cinnamon-session/csm-util.c:413
msgid "Unable to start login session (and unable to connect to the X server)"
msgstr ""
"Não foi possível iniciar a sessão de login (e não foi possível conectar ao "
"servidor X)"

#: cinnamon-session/csm-xsmp-client.c:559
msgid "Remembered Application"
msgstr "Aplicativo lembrado"

#: cinnamon-session/csm-xsmp-client.c:1198
msgid "This program is blocking logout."
msgstr "Este aplicativo está bloqueando o encerramento da sessão."

#: cinnamon-session/csm-xsmp-server.c:340
msgid ""
"Refusing new client connection because the session is currently being shut "
"down\n"
msgstr ""
"Recusando a nova conexão do cliente porque a sessão está sendo finalizada "
"neste momento\n"

#: cinnamon-session/csm-xsmp-server.c:609
#, c-format
msgid "Could not create ICE listening socket: %s"
msgstr "Não foi possível criar o soquete de escuta ICE: %s"

#: cinnamon-session/main.c:243
msgid "Override standard autostart directories"
msgstr "Substituir os diretórios padrões de início automático"

#: cinnamon-session/main.c:243
msgid "AUTOSTART_DIR"
msgstr "AUTOSTART_DIR"

#: cinnamon-session/main.c:244
msgid "Session to use"
msgstr "Sessões para usar"

#: cinnamon-session/main.c:244
msgid "SESSION_NAME"
msgstr "SESSION_NAME"

#: cinnamon-session/main.c:245
msgid "Enable debugging code"
msgstr "Habilitar depuração de código"

#: cinnamon-session/main.c:246
msgid "Do not load user-specified applications"
msgstr "Não carregar aplicativos especificados pelo usuário"

#: cinnamon-session/main.c:247
msgid "Version of this application"
msgstr "Versão deste aplicativo"

# Translators: the 'fail whale' is the black dialog we show when something goes seriously wrong. http://is.gd/Q4XOeq
#: cinnamon-session/main.c:249
msgid "Show the fail whale dialog for testing"
msgstr "Mostrar o diálogo da \"falha da baleia\" para teste"

#: cinnamon-session/main.c:278
msgid " - the Cinnamon session manager"
msgstr " - O gerenciador de sessão do Cinnamon"

#: tools/cinnamon-session-quit.c:53
msgid "Log out"
msgstr "Encerrar sessão"

#: tools/cinnamon-session-quit.c:54
msgid "Power off"
msgstr "Desligar"

#: tools/cinnamon-session-quit.c:55
msgid "Reboot"
msgstr "Reiniciar"

