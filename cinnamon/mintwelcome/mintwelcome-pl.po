msgid "Documentation"
msgstr "Dokumentacja"

#: usr/lib/linuxmint/mintwelcome/mintwelcome.py:147
msgid "Help"
msgstr "Pomoc"

#: usr/lib/linuxmint/mintwelcome/mintwelcome.py:151
#: usr/share/linuxmint/mintwelcome/mintwelcome.ui.h:2
msgid "Contribute"
msgstr "Przyłącz się"

