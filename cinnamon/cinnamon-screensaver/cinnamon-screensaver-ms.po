msgid "Please enter your password..."
msgstr "Sila masukkan kata laluan..."

#: src/unlock.py:96
msgid "Unlock"
msgstr "Buka"

#: src/unlock.py:107
msgid "Switch User"
msgstr "Tukar Pengguna"

#: src/unlock.py:194
msgid "Incorrect password"
msgstr "Kata laluan tidak sah"

#: src/unlock.py:211
msgid "Checking..."
msgstr "Menyemak..."

